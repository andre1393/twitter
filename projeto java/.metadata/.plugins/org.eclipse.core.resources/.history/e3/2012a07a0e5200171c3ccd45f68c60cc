
public class Support {
	public Support(){}
	
	public static String getJson(String[] params, String[] values){
		String json = "{\n\t";
		for(int i = 0; i < params.length && i < values.length; i++)
		{
			if(i < params.length - 1 && i < values.length - 1)
			{
				json += "\"" + params[i] + "\": \"" + values[i] + "\",\n\t";
			}else{
				
			}
		}
		return "";
	}
}
